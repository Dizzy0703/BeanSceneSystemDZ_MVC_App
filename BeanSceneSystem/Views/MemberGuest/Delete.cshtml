@model MemberGuest
<h1>Member/Guest</h1>
<form method="post" asp-action="Delete">

    <div class="border p-3 mt-4">
        <div class="row pb-2">
            <h1 class="text-primary">Delete Member/Guest</h1>
        </div>
        <div class="mb-2">
            <label>Id</label>
            <input asp-for="Id" disabled class="form-control" />
        </div>
        <div class="mb3">x
            <label>First Name</label>
            <input asp-for="FirstName" disabled class="form-control"/>
        </div>
        <div class="mb3">x
            <label>Last Name</label>
            <input asp-for="LastName" disabled class="form-control"/>
        </div>
        <div class="mb3">x
            <label>Email</label>
            <input asp-for="Email" disabled class="form-control"/>
        </div>
        <div class="mb3">x
            <label>Phone Number</label>
            <input asp-for="PhoneNo" disabled class="form-control"/>
        </div>
        <div class="mb3">x
            <label>Type</label>
            <input asp-for="Type" disabled class="form-control"/>
        </div>
        <br />
        <button type="submit" class="btn btn-primary">Delete</button>
        <a asp-controller="MemberGuest" asp-action="Index" class="btn btn-primary">Back To List</a>
    </div>
</form>

